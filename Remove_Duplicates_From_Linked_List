class LinkedList:
    def __init__(self, value):
        self.value = value
        self.next = None

# O(n) | O(1) space
def removeDuplicatesFromLinkedList(linkedList):1->3>3>4
    # Write your code here.
	currentNode  = linkedList #1
	while currentNode is not None: 
		nextDistinctNode = currentNode.next #3
		while nextDistinctNode is not None and nextDistinctNode.value == currentNode.value:
			nextDistinctNode = nextDistinctNode.next
		currentNode.next = nextDistinctNode # second 3 
		currentNode = nextDistinctNode #first 
	return linkedList
			
	
		
		
	
